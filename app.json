{
  "expo": {
    "name": "VNeID Voice Demo",
    "slug": "vneid-voice-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vneid.voicedemo",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "VNeID can truy cap micro de ghi am giong noi cua ban",
        "NSSpeechRecognitionUsageDescription": "VNeID can nhan dang giong noi de ho tro dien form tu dong",
        "UIBackgroundModes": [
          "audio",
          "voip"
        ]
      }
    },
    "android": {
      "package": "com.vneid.voicedemo",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "VNeID can truy cap micro de ghi am giong noi cua ban."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c141c377-c554-4432-9a1d-1c25038f7ba0"
      }
    }
  }
}
